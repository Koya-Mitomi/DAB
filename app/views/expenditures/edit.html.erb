<% provide(:title, '支出科目名の編集') %>
<h1>支出科目名の編集</h1>
<%= form_with(model: @expenditure, url: expenditure_path(@expenditure), method: :patch) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name, "支出科目名" %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.submit "変更を保存する", class: "btn btn-primary" %>
<% end %>
