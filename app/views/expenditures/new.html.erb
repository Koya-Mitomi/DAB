<% provide(:title, '支出科目の追加') %>
<h1>支出科目の追加</h1>
<%= form_with(model: @expenditure) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name, "支出科目名" %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.submit "支出科目を追加する", class: "btn btn-primary" %>
<% end %>
